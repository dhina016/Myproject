{
  "manifest_version": 2,

  "name": "Web Time Calculator",
  "description": "To know how long i here!!",
  "version": "1.7",

  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "clock128.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "browser-action-38.png"
    }
  },
  "background": {
    "scripts": [
      "common.js",
      "background.js"
      ],
    "persistent": true
  },
  "options_page": "options.html"
}
